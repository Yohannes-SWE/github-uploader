{
  "name": "github-uploader-client",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.15.0",
    "axios": "^1.6.7",
    "find-process": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "NODE_OPTIONS='--no-deprecation' react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build && npm run analyze:size",
    "build:dev": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "analyze:size": "npx webpack-bundle-analyzer build/static/js/*.js --no-open --report --mode=static --report build/bundle-report.html",
    "preload-critical": "npm run build && npx critical --src build --dest build --inline --minify --extract --width 1300 --height 900",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "critical": "^6.1.0"
  }
}
